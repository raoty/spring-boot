CREATE TABLE menu
(
  menuid    INT AUTO_INCREMENT
    PRIMARY KEY,
  type      VARCHAR(50)  NOT NULL,
  name      VARCHAR(100) NOT NULL,
  `key`     VARCHAR(50)  NULL,
  url       VARCHAR(200) NULL,
  media_id  VARCHAR(100) NULL,
  appid     VARCHAR(100) NULL,
  pagepath  VARCHAR(200) NULL,
  firstmenu INT          NULL
)
  ENGINE = InnoDB;

